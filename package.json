{
   "name": "clean-architecture-boilerplate",
   "version": "0.1.0",
   "description": "Examples for using clean architecture with typescript and expressjs",
   "author": "leolana",
   "license": "MIT",
   "engines": {
      "node": ">= 10.15.0"
   },
   "scripts": {
      "start": "nps",
      "test": "npm start test",
      "build": "npm start build",
      "setup": "npm start config && npm start setup.script",
      "commit": "npx git-cz",
      "precommit": "npm run lint",
      "commitmsg": "npx commitlint -e $GIT_PARAMS",
      "prettier": "npx prettier --write --"
   },
   "dependencies": {
      "body-parser": "^1.18.3",
      "chalk": "^2.4.1",
      "express": "^4.16.4",
      "figlet": "^1.2.1",
      "helmet": "^3.15.0",
      "inversify": "^5.0.1",
      "inversify-binding-decorators": "^4.0.0",
      "inversify-express-utils": "^6.2.0",
      "inversify-logger-middleware": "^3.1.0",
      "jsonfile": "^5.0.0",
      "morgan": "^1.9.1",
      "pg": "^7.7.1",
      "reflect-metadata": "^0.1.12",
      "sequelize": "^4.42.0",
      "sequelize-typescript": "^0.6.6"
   },
   "devDependencies": {
      "@types/body-parser": "^1.17.0",
      "@types/chalk": "^2.2.0",
      "@types/express": "^4.16.0",
      "@types/helmet": "0.0.42",
      "@types/jest": "^23.3.12",
      "@types/morgan": "^1.7.35",
      "@types/sequelize": "^4.27.33",
      "commitizen": "^3.0.5",
      "commitlint": "^7.2.1",
      "cz-conventional-changelog": "^2.1.0",
      "husky": "^1.3.1",
      "jest": "^23.6.0",
      "lint-staged": "^8.1.0",
      "nodemon": "^1.18.9",
      "nps": "^5.9.3",
      "nps-utils": "^1.7.0",
      "prettier": "^1.15.3",
      "rimraf": "^2.6.2",
      "sequelize-cli": "^5.4.0",
      "ts-jest": "^23.10.5",
      "ts-node": "^7.0.1",
      "tslint": "^5.12.0",
      "tslint-config-airbnb": "^5.11.1",
      "tslint-config-prettier": "^1.17.0",
      "tslint-microsoft-contrib": "^6.0.0",
      "typescript": "^3.2.2"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npx lint-staged",
         "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
      }
   },
   "config": {
      "commitizen": {
         "path": "cz-conventional-changelog"
      }
   },
   "lint-staged": {
      "*.ts": [
         "npx tslint --project tsconfig.json --format stylish --fix",
         "git add"
      ]
   }
}
